{
  "name": "unofficialmc2/instance-resolver",
  "description": "Resolver d'instance, auto link",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Fabien Sanchez",
      "email": "fabien.sanchez@arche-mc2.fr"
    }
  ],
  "minimum-stability": "stable",
  "require": {
    "php": "^8.3",
    "psr/container": "^2.0"
  },
  "require-dev": {
    "pimple/pimple": "^3.2",
    "phpunit/phpunit": "^11.3",
    "squizlabs/php_codesniffer": "^3.5",
    "phpstan/phpstan": "^1.8"
  },
  "autoload": {
    "psr-4": {
      "InstanceResolver\\": "./src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Test\\": "./test"
    }
  },
  "scripts": {
    "test": "phpunit",
    "check": [
      "phpcs",
      "phpstan analyse"
    ]
  }
